<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".fragment.CustomActivityListDialog"
    app:boxedEdges="all">
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/activityListConstraint"
    android:background="@drawable/bg_listdialog"
    android:padding="@dimen/big_padding"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center_horizontal">
    <com.google.android.material.button.MaterialButton
        android:id="@+id/home_action1_btn"
        style="@style/Widget.MyApp.MyIconButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/action_homebtn_long_press"
        android:text="@string/action_new_workout"
        app:icon="@drawable/ic_dumbbell_pair"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/home_action2_btn"
        style="@style/Widget.MyApp.MyIconButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/action_homebtn_long_press"
        android:text="@string/action_new_template"
        app:icon="@drawable/ic_favlist"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/home_action1_btn" />
    <com.google.android.material.button.MaterialButton
        android:id="@+id/home_action3_btn"
        style="@style/Widget.MyApp.MyIconButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/action_homebtn_long_press"
        android:text="@string/action_new_cardio"
        app:icon="@drawable/ic_trail_running_shoe_white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/home_action2_btn"/>
    <com.google.android.material.button.MaterialButton
        android:id="@+id/home_action4_btn"
        style="@style/Widget.MyApp.MyIconButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/action_homebtn_long_press"
        android:text="@string/action_new_run"
        app:icon="@drawable/ic_running_white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/home_action3_btn"/>
    <com.google.android.material.button.MaterialButton
        android:id="@+id/home_action5_btn"
        style="@style/Widget.MyApp.MyIconButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/action_new_sport"
        android:contentDescription="@string/action_homebtn_long_press"
        app:icon="@drawable/ic_basketball_player_with_the_ball_white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/home_action4_btn"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/home_action6_image"
        style="@style/Widget.MyApp.MyIconButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/action_homebtn_long_press"
        android:text="@string/action_new_winter"
        app:icon="@drawable/ic_winter_snow_flake_white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/home_action5_btn"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/home_action7_btn"
        style="@style/Widget.MyApp.MyIconButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/action_new_water"
        android:text="@string/action_new_water"
        app:icon="@drawable/ic_swimmer_diving_to_water_white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/home_action6_image"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/home_action8_btn"
        style="@style/Widget.MyApp.MyIconButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/action_new_bike"
        android:text="@string/action_new_bike"
        app:icon="@drawable/ic_bike_white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/home_action7_btn"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/home_action9_btn"
        style="@style/Widget.MyApp.MyIconButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/action_new_misc"
        android:text="@string/action_new_misc"
        app:icon="@drawable/ic_dancer_with_music_white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/home_action8_btn"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/home_action10_btn"
        style="@style/Widget.MyApp.MyIconButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/action_new_misc"
        android:text="@string/label_activity"
        app:icon="@drawable/ic_sort_a_to_z_white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/home_action9_btn"/>

</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>