<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homePageFragment">
    <action android:id="@+id/action_global_homePageFragment" app:destination="@id/homePageFragment" />
    <action android:id="@+id/action_global_liveFragment"
        app:popUpTo="@id/homePageFragment" />
    <fragment
        android:id="@+id/homePageFragment"
        android:name="com.a_track_it.fitdata.fragment.HomePageFragment"
        android:label="fragment_home_relative"
        tools:layout="@layout/fragment_home_relative" >
        <action
            android:id="@+id/action_homePageFragment_to_sessionEntryFragment"
            app:destination="@id/sessionEntryFragment"
            app:launchSingleTop="false"
            app:popUpTo="@id/homePageFragment"
            app:popUpToInclusive="true" />
        <deepLink
            android:id="@+id/deepLink3"
            app:uri="www.a-track-it.com/home" />
    </fragment>
    <fragment
        android:id="@+id/sessionEntryFragment"
        android:name="com.a_track_it.fitdata.fragment.SessionEntryFragment"
        android:label="fragment_session_entry"
        tools:layout="@layout/fragment_session_entry" >
        <action
            android:id="@+id/action_sessionEntryFragment_to_liveFragment"
            app:destination="@id/liveFragment"
            app:launchSingleTop="false"
            app:popUpTo="@+id/homePageFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/liveFragment"
        android:name="com.a_track_it.fitdata.fragment.LiveFragment"
        android:label="fragment_live_relative"
        tools:layout="@layout/fragment_live_relative" >
        <deepLink
            android:id="@+id/deepLink2"
            app:uri="www.a-track-it.com/live/{id}" />
        <action
            android:id="@+id/action_liveFragment_to_endOfSetFragment"
            app:destination="@id/endOfSetFragment" />
        <action
            android:id="@+id/action_liveFragment_to_sessionReportFragment"
            app:destination="@id/sessionReportFragment" />
    </fragment>
    <fragment
        android:id="@+id/sessionReportFragment"
        android:name="com.a_track_it.fitdata.fragment.SessionReportFragment"
        android:label="fragment_session_report"
        tools:layout="@layout/fragment_session_report" >
        <argument
            android:name="ICON_ID"
            app:argType="integer"
            android:defaultValue="0" />
        <argument
            android:name="COLOR_ID"
            app:argType="integer"
            android:defaultValue="0" />
        <argument
            android:name="WORKOUT_ID"
            app:argType="long"
            android:defaultValue="0L" />
        <argument
            android:name="TIMED_ID"
            app:argType="long"
            android:defaultValue="0L" />
        <deepLink
            android:id="@+id/deepLink"
            app:uri="www.a-track-it.com/report/{id}" />
    </fragment>
    <fragment
        android:id="@+id/endOfSetFragment"
        android:name="com.a_track_it.fitdata.fragment.EndOfSetFragment"
        android:label="fragment_session_finalise"
        tools:layout="@layout/fragment_session_finalise" >
        <action
            android:id="@+id/action_endOfSetFragment_to_liveFragment"
            app:destination="@id/liveFragment"
            app:launchSingleTop="false" />
        <action
            android:id="@+id/action_endOfSetFragment_to_sessionReportFragment"
            app:destination="@id/sessionReportFragment" />
    </fragment>
    <dialog
        android:id="@+id/customListFragment"
        android:name="com.a_track_it.fitdata.fragment.CustomListFragment"
        android:label="fragment_customlist"
        tools:layout="@layout/fragment_customlist" >
        <argument
            android:name="ARG_TYPE"
            app:argType="integer" />
        <argument
            android:name="ARG_PRESET"
            app:argType="integer" />
    </dialog>

    <dialog
        android:id="@+id/customScoreDialogFragment"
        android:name="com.a_track_it.fitdata.fragment.CustomScoreDialogFragment"
        tools:layout="@layout/fragment_customscore"
        android:label="CustomScoreDialogFragment" >
        <argument
            android:name="ARG_TYPE"
            app:argType="integer"
            android:defaultValue="0" />
        <argument
            android:name="ARG_PRESET"
            app:argType="integer"
            android:defaultValue="0" />
    </dialog>
    <dialog
        android:id="@+id/customConfirmDialog"
        android:name="com.a_track_it.fitdata.fragment.CustomConfirmDialog"
        android:label="dialog_confirmation"
        tools:layout="@layout/dialog_confirmation" />


</navigation>