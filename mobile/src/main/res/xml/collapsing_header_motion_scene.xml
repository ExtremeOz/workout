<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:motion="http://schemas.android.com/tools">

    <ConstraintSet android:id="@+id/header_start">
        <Constraint android:id="@+id/collapsing_toolbar">
            <CustomAttribute  app:attributeName="layout_height"
                app:customFloatValue="250" />
        </Constraint>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/header_end" motion:deriveConstraintsFrom="@id/header_start">
        <Constraint android:id="@+id/collapsing_toolbar">
            <CustomAttribute  app:attributeName="layout_height"
                app:customFloatValue="500" />
        </Constraint>

    </ConstraintSet>
 
</MotionScene>
